{
  "name": "@material-wc/core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": {
      "types": "./dist/styles/index.d.ts",
      "import": "./dist/styles/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js"
    },
    "./icon": {
      "types": "./dist/icon/icon.d.ts",
      "import": "./dist/icon/icon.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:css && vite build",
    "build:css": "node ../../tools/css-builder/build.js --package=core",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
